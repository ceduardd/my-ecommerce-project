{
  "name": "my-ecommerce-project",
  "version": "1.0.0",
  "description": "My first eCommerce project with MERN Stack",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server --exec babel-node",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "babel-node backend/seeder",
    "data:destroy": "babel-node backend/seeder --destroy"
  },
  "author": "Eduardo Chavez",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6"
  }
}
